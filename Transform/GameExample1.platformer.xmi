<?xml version="1.0" encoding="UTF-8"?>
<platformer:Game xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:platformer="http://www.example.org/platformer" currentLevelCounter="1" friction="10" gravity="980" screen="pygame.display.set_mode((SCREEN_WIDTH, SCREEN_HEIGHT))" clock="pygame.time.Clock()">
  <levels width="2000" height="200">
    <entities xsi:type="platformer:Player" size="50">
      <position x="50"/>
      <sprite path="player.png"/>
    </entities>
    <entities xsi:type="platformer:Platform" direction="RIGHT">
      <position/>
      <sprite path="platform.png"/>
    </entities>
    <entities xsi:type="platformer:Item">
      <position/>
      <sprite path="item.png"/>
    </entities>
    <background path="level.png"/>
    <winCoordinate x="1000"/>
  </levels>
  <ui>
    <button label="Reset"/>
    <button label="QUIT" action="EXIT_GAME"/>
    <button label="Menu" action="OPEN_MENU"/>
    <button label="Close" action="CLOSE_MENU"/>
  </ui>
  <lives amount="3"/>
  <score/>
</platformer:Game>
